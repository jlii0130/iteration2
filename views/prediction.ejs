                    <div class="w3-white">
                      <h3>Tell us your location you wish to farm</h3>
                      <p>Choose from the below options. (1/3)</p>
                      <select class="w3-full" id="selectarea" required>
                        <option value="none" selected disabled hidden>Select from the options</option>
                        <option value="Broome">Broome</option>
                        <option value="Bunbury">Bunbury</option>
                        <option value="Carnarvon">Carnarvon</option>
                        <option value="Donnybrook">Donnybrook</option>
                        <option value="Dwellingup">Dwellingup</option>
                        <option value="Geraldton">Geraldton</option>
                        <option value="Kununurra">Kununurra</option>
                        <option value="Manjimup">Manjimup</option>
                        <option value="Merredin">Merredin</option>
                        <option value="Perth">Perth</option>
                        <option value="Perth Hills">Perth Hills</option>
                      </select>

                    </div>
                    <!--buffer-->
